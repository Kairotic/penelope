


-- Transposition

d1 $ chunk 4 (|+| n 12) $ jux rev $ slow 2 $ n (off 0.125 (+7) $ slow 4 $ "e(3,8) c(3,8) c(3,8) e*2 f(3,8) d*2 d(3,8) g*2" + "<7 2 12>") # sound "supermandolin"
  # legato 4
  # room 0.8
  # size 0.9

d2 $ n "0 .. 7" # sound "amencutup"
  # orbit 1
  # gain 1.2
  |+| n "<0 4 8 16>"

v2 $ rev "white blue"